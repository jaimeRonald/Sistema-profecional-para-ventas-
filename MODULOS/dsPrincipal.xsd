                    <CommandText>SELECT        
FROM            Clientes INNER JOIN
                         Factura ON Clientes.idCliente = Factura.id_cliente INNER JOIN
                         Detalle ON Factura.idFactura = Detalle.idFactura INNER JOIN
                         ModoPago ON Factura.id_ModoPago = ModoPago.idModoPago INNER JOIN
                         Producto ON Detalle.idProducto = Producto.idProducto INNER JOIN
                         Usuarios ON Factura.id_usuario = Usuarios.idUsuario INNER JOIN
                         Clientes AS Clientes_1 ON Factura.id_cliente = Clientes_1.idCliente INNER JOIN
                         Detalle AS Detalle_1 ON Factura.idFactura = Detalle_1.idFactura AND Producto.idProducto = Detalle_1.idProducto INNER JOIN
                         Factura AS Factura_1 ON Clientes.idCliente = Factura_1.id_cliente AND Detalle.idFactura = Factura_1.idFactura AND ModoPago.idModoPago = Factura_1.id_ModoPago AND 
                         Usuarios.idUsuario = Factura_1.id_usuario AND Clientes_1.idCliente = Factura_1.id_cliente AND Detalle_1.idFactura = Factura_1.idFactura INNER JOIN
                         ModoPago AS ModoPago_1 ON Factura.id_ModoPago = ModoPago_1.idModoPago AND Factura_1.id_ModoPago = ModoPago_1.idModoPago INNER JOIN
                         Producto AS Producto_1 ON Detalle.idProducto = Producto_1.idProducto AND Detalle_1.idProducto = Producto_1.idProducto INNER JOIN
                         Usuarios AS Usuarios_1 ON Factura.id_usuario = Usuarios_1.idUsuario AND Factura_1.id_usuario = Usuarios_1.idUsuario</CommandText>
